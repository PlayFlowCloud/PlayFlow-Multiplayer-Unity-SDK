          "parameters": [
            {
              "name": "api-key",
              "in": "header",
              "required": true,
              "schema": {
                "type": "string",
                "description": "PlayFlow API Key",
                "title": "Api-Key"
              },
              "description": "PlayFlow API Key"
            }
          ],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ServerCreateRequest",
